{
    "table": "users",
    "action": "create",
    "payload": {
        "name": {
            "type": "String",
            "constraints": [
                "NotNull",
                "Immutable"
            ]
        },
        "email": {
            "type": "Email",
            "constraints": [
                "Unique"
            ]
        },
        "age": {
            "type": "Number",
            "constraints": [
                "NotNull"
            ]
        }
    }
}